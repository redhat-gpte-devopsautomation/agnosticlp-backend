apiVersion: build.openshift.io/v1
kind: BuildConfig
metadata:
  labels:
    build: classroom
  name: classroom
spec:
  output:
    to:
      kind: ImageStreamTag
      name: GUID-classroom:latest
  postCommit: {}
  resources: {}
  runPolicy: Serial
  source:
    binary: {}
    type: Binary
  strategy:
    sourceStrategy:
      from:
        kind: ImageStreamTag
        name: jboss-eap71-openshift:1.4
        namespace: openshift
    type: Source
  successfulBuildsHistoryLimit: 5